    "load_dotenv()\n",
    "\n",
    "# Read CSV file\n",
    "df = pd.read_csv(os.environ['TRAIN_LOCATION'])\n",
    "\n",
    "# Perform one-hot encoding on 'City'\n",
    "df = pd.get_dummies(df, columns=['City'])\n",
    "\n",
    "# View result\n",
    "df.info()\n",
    "\n"
